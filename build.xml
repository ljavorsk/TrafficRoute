<?xml version="1.0" encoding="UTF-8"?>
<project default="run" name="TraficRoute Semestral project for IJA">
    <target name="run" depends="compile">
        <java classname="Main">
            <classpath path="build"/>
        </java>
    </target>
    <target name="compile">
        <javac target="1.8" source="1.8" classpath="lib/json-simple-1.1.jar" includeantruntime="false" srcdir="./src"  destdir="build"/>
    </target>
</project>

